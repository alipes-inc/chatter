{
    "name": "alipes/chatter",
    "description": "A simple forum package for your laravel app",
    "authors": [
        {
            "name": "Tony Lea",
            "email": "tony@devdojo.com"
        },
        {
            "name": "Nathan Lamont",
            "email": "nathan@alipes.com"
        }
    ],
    "require": {
        "mews/purifier": "~3.0"
    },
    "require-dev": {
        "illuminate/cache": "~5.1.20|5.2.*|5.3.*|~6",
        "illuminate/console": "~5.1.20|5.2.*|5.3.*|~6",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~5.7.14|~7"
    },
    "autoload": {
	    "psr-4": {
	      "Alipes\\Chatter\\": "/src"
		}
	},
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alipes\\Chatter\\ChatterServiceProvider"
            ]
        }
    }
}
